{
  "name": "jsbarcode",
  "version": "4.0.0-alpha.5",
  "description": "JsBarcode is a customizable barcode generator with support for multiple barcode formats.",
  "main": "./lib",
  "directories": {
    "example": "example",
    "test": "test",
    "lib": "src"
  },
  "scripts": {
    "test": "npm run convert && mocha ./test/node",
    "coveralls": "NODE_ENV=test YOURPACKAGE_COVERAGE=1 ./node_modules/.bin/mocha test/node/ --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "npm run convert && nyc --reporter=html mocha test/node/",
    "convert": "babel src --config-file ./.babelrc --source-maps -d lib",
    "build": "npm run convert && webpack ./example/index.js",
    "format": "prettier-eslint --write ./src/**/*.js ./test/node/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lindell/JsBarcode.git"
  },
  "keywords": [
    "barcode",
    "canvas",
    "code128",
    "upc",
    "ean",
    "itf",
    "msi",
    "pharmacode"
  ],
  "author": "Johan Lindell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lindell/JsBarcode/issues"
  },
  "homepage": "https://github.com/lindell/JsBarcode#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "blanket": "^1.2.3",
    "canvas": "^2.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "prettier-eslint-cli": "^4.7.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "xmldom": "^0.1.31"
  },
  "typings": "./jsbarcode.d.ts"
}
